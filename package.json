    "react-select": "^1.0.0-rc.10",
    "react-select-search": "^0.5.6",
    "react-toastr": "^2.9.5",
    "redux": "^3.7.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.2",
    "sequelize": "^4.8.0",
