{
  "name": "postit-app",
  "version": "1.0.0",
  "description": "Postit is a group messaging app for any group individuals with similar interest ",
  "main": "index.js",
  "scripts": {
    "migrate": "sequelize db:migrate",
    "unmigrate": "sequelize db:migrate:undo:all",
    "start:dev": "nodemon --exec babel-node server/app.js",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "node_modules/.bin/esw webpack.config.* server client",
    "test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && nyc --reporter=html --reporter=text mocha --compilers js:babel-register --timeout 10000 ",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "clean:html": "babel-node tools/buildHtml.js",
    "build:html": "npm run clean:html",
    "heroku-postbuild": "babel-node tools/build.js && npm run build:html",
    "start": "babel-node tools/distServer.js"
  },
  "keywords": [
    "PostIt",
    "Group",
    "Messaging"
  ],
  "author": "Aaron Biliyok",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.8.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-watch": "^3.1.2",
    "expect": "^21.2.1",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.2.1",
    "webpack-hot-middleware": "^2.19.1"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.2",
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.1.2",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.16.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "font-loader": "^0.1.2",
    "fs": "0.0.1-security",
    "fuse": "^0.4.0",
    "jquery": "^3.2.1",
    "jsonwebtoken": "^8.1.0",
    "jwt-decode": "^2.2.0",
    "materialize-css": "^0.100.2",
    "md5": "^2.2.1",
    "morgan": "^1.9.0",
    "nodemailer": "^4.2.0",
    "npm-run-all": "^4.1.1",
    "open": "0.0.5",
    "path": "^0.12.7",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.0",
    "randomstring": "^1.1.5",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-materialize": "^1.0.16",
    "react-redux": "^5.0.6",
    "react-router": "^3.2.0",
